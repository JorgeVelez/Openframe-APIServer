{
  "name": "OpenframeUser",
  "plural": "OpenframeUsers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "bio": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "full_name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owned_frames": {
      "type": "hasMany",
      "model": "Frame",
      "foreignKey": "ownerId"
    },
    "managed_frames": {
      "type": "hasAndBelongsToMany",
      "model": "Frame"
    },
    "collections": {
      "type": "hasMany",
      "model": "Collection",
      "foreignKey": "ownerId"
    },
    "owned_artwork": {
      "type": "hasMany",
      "model": "Artwork",
      "foreignKey": "ownerId"
    },
    "authored_artwork": {
      "type": "hasMany",
      "model": "Artwork",
      "foreignKey": "authorId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
