{
  "name": "OpenframeUser",
  "plural": "OpenframeUsers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "protected": [
    "email",
    "full_name",
    "created",
    "modified"
  ],
  "properties": {
    "bio": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "full_name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owned_frames": {
      "type": "hasMany",
      "model": "Frame",
      "foreignKey": "ownerId"
    },
    "managed_frames": {
      "type": "hasAndBelongsToMany",
      "model": "Frame"
    },
    "collections": {
      "type": "hasMany",
      "model": "Collection",
      "foreignKey": "ownerId"
    },
    "owned_artwork": {
      "type": "hasMany",
      "model": "Artwork",
      "foreignKey": "ownerId"
    },
    "authored_artwork": {
      "type": "hasMany",
      "model": "Artwork",
      "foreignKey": "authorId"
    },
    "identities": {
      "type": "hasMany",
      "model": "OpenframeUserIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "OpenframeUserCredential",
      "foreignKey": "userId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "OpenframeAccessToken",
      "foreignKey": "userId"
    },
    "roleMappings": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId"
    }
  },
  "acls": [{
    "accessType": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  }, {
    "accessType": "EXECUTE",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "ALLOW",
    "property": ["find", "findById", "findOne"]
  }, {
    "accessType": "READ",
    "principalType": "ROLE",
    "principalId": "$owner",
    "permission": "ALLOW"
  }, {
    "accessType": "WRITE",
    "principalType": "ROLE",
    "principalId": "$owner",
    "permission": "ALLOW"
  }, {
    "accessType": "EXECUTE",
    "principalType": "ROLE",
    "principalId": "$owner",
    "permission": "ALLOW",
    "property": ["all_frames"]
  }],
  "methods": {}
}

