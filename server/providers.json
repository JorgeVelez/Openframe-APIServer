{
    "local": {
        "provider": "local",
        "module": "passport-local",
        "usernameField": "username",
        "passwordField": "password",
        "authPath": "/auth/local",
        "successRedirect": "/login-success",
        "failureRedirect": "/login",
        "failureFlash": true,
        "setAccessToken" : true,
        "session" : true
    }
}

